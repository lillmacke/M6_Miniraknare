import sympy as sp

def beräkna(uttryck):
    try:
        expr = sp.sympify(uttryck)
        resultat = expr.evalf()
        return resultat
    except Exception as e:
        print("Fel i uttrycket: ", e)
        return None
    
uttryck = input("Skriv ett uttryck: ")
resultat = beräkna(uttryck)
print(f"Resultat: {resultat:.3f}")